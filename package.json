{
	"name": "bangtastic",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"process-bangs": "esrun ./bangs/process-bangs.ts > ./bangs/data/bang.merged.json",
		"download-bangs": "curl duckduckgo.com/bang.js | jq > ./bangs/data/duckduckgo/bang.json"
	},
	"devDependencies": {
		"@picocss/pico": "2.0.3",
		"@stenway/oml": "^1.1.0",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/callsite": "^1.0.34",
		"@types/debug": "^4.1.12",
		"@types/eslint": "8.56.3",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.11.20",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vincjo/datatables": "^1.14.5",
		"debug": "^4.3.4",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"esrun": "^3.2.26",
		"lodash-es": "^4.17.21",
		"normalize-url": "^8.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"sass": "^1.71.1",
		"svelte": "5.0.0-next.68",
		"svelte-check": "^3.6.4",
		"tidy-url": "^1.11.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.4"
	},
	"type": "module"
}
